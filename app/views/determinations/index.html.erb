<p id="notice"><%= notice %></p>

<h1>Determinations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @determinations.each do |determination| %>
      <tr>
        <td><%= link_to 'Show', determination %></td>
        <td><%= link_to 'Edit', edit_determination_path(determination) %></td>
        <td><%= link_to 'Destroy', determination, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Determination', new_determination_path %>
